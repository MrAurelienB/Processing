class Pair_c
{

  private float m_First, m_Second;
  
  Pair_c(float x, float y)
    {
    this.m_First = x;
    this.m_Second = y;
    } // Pair_c()
    
  public float First()
    {
    return (this.m_First);
    } // X()
    
  public float Second()
    {
    return (this.m_Second);
    } // Y()

} // class Pair_c
